.nav-tab{
    margin: auto;
    text-align: center;
    color: #fff;
    font-size: 36rpx;
    height: 170rpx;
    display: flex;
    position: fixed;
    top:0;
    left: 0;
    right: 0;
    z-index: 888;
}
.background1{
    background: #ffffff;
}
.background2{
    background: #000000;
}
.background3{
    background: transparent;
}
.nav-tab .top-left{
    position: absolute;
    left: 30rpx;
    /* top: 50%;
    transform: translateY(-50%); */
}
.title-panel{
    flex: 1;
    margin: auto;
    text-align: center;
    min-width: 0;
}
.title{
    width: 40%;
    text-align: center;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    margin: auto;
}
.title1{
    color: #252525;
}
.title2{
    color: #fff;
}
.navigation-panel{
    border-radius: 40rpx;
    display: flex;
    padding: 13rpx 0 ;
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    box-sizing: border-box;
}
.navigationBorder{
    border: 1px solid #DFDFDF;
}
.navigation-item{
    width: 87rpx;
    margin: auto;
    display: flex;
    align-items: center;
    /* justify-content: center; */
    height: 28rpx;
    box-sizing: border-box;
    flex: 1;
}
.navigation-item image{
    width: 32rpx;
    height: 32rpx;
}
.dividingLine {
    border-right: 1rpx solid #D0D0D0;
}

.navigationBorder .navigation-item image{
    width: 29rpx;
    height: 29rpx;
}
.navigationBorder .navigation-item{
    justify-content: center;
}
