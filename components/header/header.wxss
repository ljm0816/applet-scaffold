/* components/header/header.wxss */
.header{
  position: fixed;
  top: 0;
  background: #fff;
  width: 100%;
  border-bottom: 1rpx solid red;
}
.header .title{
  position: relative;
  text-align: center;
  max-width: 400rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 10rpx;
  margin: auto;
  font-size: 34rpx;
  color: #000;
  font-weight: 600;
}
.icon-list{
  position: absolute;
  z-index: 100;
  bottom: 10rpx;
  display: flex;
  line-height: 66rpx;
  margin-left: 10rpx; 
}
.borderB{
  padding: 0 20rpx;
  border: 1rpx solid #e5e5e5;
  border-radius: 100rpx;
}
.header image{
  display: block;
}



.nav-wrap {
  position: fixed;
  width: 100%;
  top: 0;
  background: #fff;
  color: #000;
  z-index: 9999999;
}
/* 标题要居中 */
.nav-title {
  position: absolute;
  text-align: center;
  max-width: 400rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  font-size: 34rpx;
  color: #2c2b2b;
  font-weight: 600;
}

.nav-capsule {
  display: flex;
  align-items: center;
  margin-left: 10rpx;
  max-width: 80rpx;
  justify-content: space-between;
  height: 100%;
}

.navbar-v-line {
  width: 1px;
  height: 45rpx;
  background-color: #e5e5e5;
  margin: 0 20rpx;
}

.back-pre, .back-home {
  width: 32rpx;
   height: 36rpx;
  margin-top: 4rpx;
  padding: 10rpx;
}
.nav-capsule .back-home {
  width: 36rpx;
  height: 40rpx;
  margin-top: 3rpx;
}
.nav-capsule image{
  display: block;
}
.nav-list{
  
  display: flex;
  justify-content: space-between;
  padding: 10rpx 20rpx;
}
.navborder{
  border: 1rpx solid #e5e5e5;
  border-radius: 100rpx;
}